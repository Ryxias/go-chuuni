{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-jsx", "standard-react"],
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "react/no-unused-prop-types": "off",
    "react/prop-types": "off",
    "react/jsx-indent-props": ["error", 4],
    "jsx-quotes": ["error", "prefer-double"],
    "indent": [
      "error", 2, {
      "ignoredNodes": [
        "JSXElement *"
      ]
    }]
  }
}
